{
  "expo": {
    "name": "Music Pad",
    "slug": "music-pad",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android"],
    "version": "0.1.4",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
